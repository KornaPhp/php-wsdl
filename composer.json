{
    "name": "php-soap/wsdl",
    "description": "Deals with WSDLs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Soap\\Wsdl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoapTest\\Wsdl\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toonverwerft@gmail.com"
        }
    ],
    "bin": [
        "bin/wsdl"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
        "ext-dom": "*",
        "azjezz/psl": "^3.0 || ^4.0",
        "league/uri": "^7.0",
        "league/uri-components": "^7.0",
        "php-soap/xml": "^1.9",
        "symfony/console": "^5.4 || ^6.0 || ^7.0",
        "veewee/xml": "~3.0"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.88",
        "phpunit/phpunit": "^12.3",
        "psalm/plugin-symfony": "^5.0",
        "vimeo/psalm": "^6.13"
    }
}
